{"code":"import url from 'url';\r\nexport function parseUrl(siteUrl) {\r\n    const { hostname, port, protocol } = url.parse(siteUrl);\r\n    if (!port) {\r\n        if (protocol === 'http:') {\r\n            return {\r\n                host: hostname,\r\n                port: 80,\r\n            };\r\n        }\r\n        if (protocol === 'https:') {\r\n            return {\r\n                host: hostname,\r\n                port: 443,\r\n            };\r\n        }\r\n        throw new Error(`Cant find port in url ${siteUrl}`);\r\n    }\r\n    return {\r\n        host: hostname,\r\n        port: Number.parseInt(port, 10),\r\n    };\r\n}\r\n//# sourceMappingURL=url.js.map","map":"{\"version\":3,\"file\":\"url.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../src/utils/url.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,GAAG,MAAM,KAAK,CAAC;AAEtB,MAAM,UAAU,QAAQ,CAAC,OAAe;IACtC,MAAM,EAAE,QAAQ,EAAE,IAAI,EAAE,QAAQ,EAAE,GAAG,GAAG,CAAC,KAAK,CAAC,OAAO,CAAC,CAAC;IAExD,IAAI,CAAC,IAAI,EAAE;QACT,IAAI,QAAQ,KAAK,OAAO,EAAE;YACxB,OAAO;gBACL,IAAI,EAAE,QAAQ;gBACd,IAAI,EAAE,EAAE;aACT,CAAC;SACH;QACD,IAAI,QAAQ,KAAK,QAAQ,EAAE;YACzB,OAAO;gBACL,IAAI,EAAE,QAAQ;gBACd,IAAI,EAAE,GAAG;aACV,CAAC;SACH;QACD,MAAM,IAAI,KAAK,CAAC,yBAAyB,OAAO,EAAE,CAAC,CAAC;KACrD;IAED,OAAO;QACL,IAAI,EAAE,QAAQ;QACd,IAAI,EAAE,MAAM,CAAC,QAAQ,CAAC,IAAI,EAAE,EAAE,CAAC;KAChC,CAAC;AACJ,CAAC\"}"}
